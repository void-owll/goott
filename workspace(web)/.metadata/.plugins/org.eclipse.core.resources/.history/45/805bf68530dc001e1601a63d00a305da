<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">

	// 산술연산자(+, -, *, /, %)
	let num1 = 15, num2 = 4, result;
	result = num1 + num2;
	document.write(num1 + " + " + num2 + " = " + result + "<br>");	
	
	result = num1 - num2;
	document.write(`${num1} - ${num2} = ${result}<br>`);
	
	
	
</script>
